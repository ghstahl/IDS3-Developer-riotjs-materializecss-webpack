{
  "name": "mdl-webpack-boilerplate",
  "title": "Material Design lite boilerplate",
  "version": "1.0.0",
  "description": "Material Design lite + Webpack",
  "main": "lib/index.js",
  "scripts": {
    "images": "cpy ./lib/images/*.* ./www/images/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle": "webpack --config webpack.config.prod.js -p -d --progress --colors",
    "build": "npm-run-all images bundle",
    "serve": "webpack-dev-server -d --content-base www/ --hot --progress --colors",
    "dev": "npm-run-all images serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shprink/mdl-webpack-boilerplate.git"
  },
  "author": "shprink <contact@julienrenaux.fr> (http://julienrenaux.fr/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shprink/mdl-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/shprink/mdl-webpack-boilerplate#readme",
  "dependencies": {
    "lodash": "^4.17.2",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.2.1",
    "npm-run-all": "^3.1.2",
    "riot": "^3.0.4",
    "riot-route": "^3.0.2",
    "riotcontrol": "0.0.3",
    "whatwg-fetch": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-riot": "^1.1.0",
    "cpy": "^5.0.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "hammerjs": "^2.0.8",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jquery": "^3.1.1",
    "materialize-css": "git+https://github.com/Dogfalo/materialize.git",
    "node-sass": "^4.0.0",
    "nprogress": "^0.2.0",
    "path": "^0.12.7",
    "resolve-url-loader": "^1.6.1",
    "riotjs-loader": "^3.0.0",
    "riotux": "^2.1.1",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.7.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
